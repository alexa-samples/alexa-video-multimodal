    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2018
    },
    "extends": [
      "standard",
      "plugin:jsdoc/recommended",
      "plugin:jasmine/recommended"
    ],
    "globals": {
      "Observable": true,
      "_babelPolyfill": true
    },
    "env": {
      "jasmine": true
    },
    "plugins": [
      "jasmine"
    ],
    "rules": {
      "jasmine/no-spec-dupes": "off"
