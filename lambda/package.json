    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@istanbuljs/nyc-config-babel": "^2.1.1",
    "@types/jasmine": "^3.3.16",
    "adm-zip": "^0.4.13",
    "babel-loader": "^8.0.6",
