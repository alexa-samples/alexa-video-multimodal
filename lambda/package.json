    "jsdoc": "^3.6.3",
    "lodash-webpack-plugin": "^0.11.5",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.3",
    "uuid": "^3.3.2",
    "webpack": "^4.44.1",
